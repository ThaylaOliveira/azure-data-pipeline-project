    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "adf-projeto-dados"
        },
        "ls_blob_bronze_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'ls_blob_bronze'"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "[concat(parameters('factoryName'), '/ls_blob_bronze')]",
            "type": "Microsoft.DataFactory/factories/linkedServices",
            "apiVersion": "2018-06-01",
            "properties": {
                "description": "ConexÃ£o com a Storage Account no Azure que armazena os arquivos CSV brutos (camada Bronze) utilizados como fonte de entrada no projeto de pipeline de dados.",
                "annotations": [],
                "type": "AzureBlobStorage",
                "typeProperties": {
                    "connectionString": "[parameters('ls_blob_bronze_connectionString')]"
                }
            },
            "dependsOn": []
        },
        {
            "name": "[concat(parameters('factoryName'), '/ds_orders_csv')]",
            "type": "Microsoft.DataFactory/factories/datasets",
            "apiVersion": "2018-06-01",
            "properties": {
                "description": "Dataset pointing to the raw orders CSV file in the Bronze layer. Used in ingestion pipeline for order data processing.",
                "linkedServiceName": {
                    "referenceName": "ls_blob_bronze",
                    "type": "LinkedServiceReference"
                },
                "parameters": {
                    "pasta_arquivo": {
                        "type": "string"
                    },
                    "nome_arquivo": {
                        "type": "string"
                    }
                },
                "annotations": [],
                "type": "DelimitedText",
                "typeProperties": {
                    "location": {
                        "type": "AzureBlobStorageLocation",
                        "fileName": {
                            "value": "@dataset().nome_arquivo",
                            "type": "Expression"
                        },
                        "folderPath": {
                            "value": "@dataset().pasta_arquivo",
                            "type": "Expression"
                        },
                        "container": "bronze"
                    },
                    "columnDelimiter": ",",
                    "escapeChar": "\\",
                    "firstRowAsHeader": true,
                    "quoteChar": "\""
                },
                "schema": [
                    {
                        "name": "order_id",
                        "type": "String"
                    },
                    {
                        "name": "customer_id",
                        "type": "String"
                    },
                    {
                        "name": "order_status",
                        "type": "String"
                    },
                    {
                        "name": "order_purchase_timestamp",
                        "type": "String"
                    },
                    {
                        "name": "order_approved_at",
                        "type": "String"
                    },
                    {
                        "name": "order_delivered_carrier_date",
                        "type": "String"
                    },
                    {
                        "name": "order_delivered_customer_date",
                        "type": "String"
                    },
                    {
                        "name": "order_estimated_delivery_date",
                        "type": "String"
                    }
                ]
            },
            "dependsOn": [
                "[concat(variables('factoryId'), '/linkedServices/ls_blob_bronze')]"
            ]
        },
